{
  "name": "apie/static-resources-plugin",
  "description": "Composer package of the apie library: static resources plugin",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\StaticResourcesPlugin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\StaticResourcesPlugin\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "apie/core": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "apie/mock-objects": "dev-main"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}